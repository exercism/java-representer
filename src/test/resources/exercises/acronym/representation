        return Arrays.stream(PLACEHOLDER_7.split("(?!')\\W")).filter(PLACEHOLDER_8 -> !PLACEHOLDER_9.isEmpty()).map(PLACEHOLDER_8 -> PLACEHOLDER_9.substring(0, 1)).collect(Collectors.joining()).toUpperCase();
