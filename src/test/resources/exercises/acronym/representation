class PLACEHOLDER_1 {

    private String PLACEHOLDER_2;

    PLACEHOLDER_1(String PLACEHOLDER_4) {
        this.PLACEHOLDER_2 = PLACEHOLDER_3(PLACEHOLDER_4);
    }

    String PLACEHOLDER_5() {
        return PLACEHOLDER_2;
    }

    private String PLACEHOLDER_3(String PLACEHOLDER_4) {
        String PLACEHOLDER_6 = PLACEHOLDER_4.replace("_", "");
        return Arrays.stream(PLACEHOLDER_6.split("(?!')\\W")).filter(PLACEHOLDER_7 -> !PLACEHOLDER_7.isEmpty()).map(PLACEHOLDER_7 -> PLACEHOLDER_7.substring(0, 1)).collect(Collectors.joining()).toUpperCase();
    }
}
